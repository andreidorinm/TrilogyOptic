{
  "name": "trilogyoptic",
  "version": "1.0.0",
  "description": "MERN Trilogy Optic App",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "yarn start --prefix frontend",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "yarn_CONFIG_PRODUCTION=false yarn install --prefix frontend && yarn build --prefix frontend"
  },
  "author": "Andrei Mihaila",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-sitemap-xml": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "typescript": "^4.6.3"
  }
}
